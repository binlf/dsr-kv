{
  "name": "dsr-kv",
  "version": "0.2.0",
  "description": "De-Serialize keys or values in a stringified JSON object.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "tsc",
    "dev": "tsup src/index.ts --watch",
    "test": "bun test ./src/_test_.ts"
  },
  "keywords": [
    "keyval",
    "object",
    "deserializer",
    "serializer",
    "json",
    "key",
    "value",
    "key-value"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/binlf/dsr-kv"
  },
  "author": "binlf",
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@types/node": "^22.5.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}